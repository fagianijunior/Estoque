<%= simple_form_for(@handling_item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :material_handling_id %>
    <%= f.input :material_id %>
    <%= f.input :quantity %>
    <%= f.input :price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
